@model IEnumerable<CourseGroupVM>

@{
    CourseVM course = ViewBag.Course as CourseVM;
    if (Model.Count() == 0) {
        return;
    }
}
<div class="tableContainer">
    <table>
        <tr>
            <th>
                Название курса
            </th>
            <th>
                Аббревиатура курса
            </th>
            <th>
                Группы
            </th>
            <th>
                Управление
            </th>
        </tr>

        @foreach (CourseGroupVM courseGroup in Model) {
            @Html.Partial("Partial/_CourseGroupRow", new CourseGroupVM { Id = courseGroup.Id, Course = course, Group = courseGroup.GetGroup() })
        }
    </table>
</div>