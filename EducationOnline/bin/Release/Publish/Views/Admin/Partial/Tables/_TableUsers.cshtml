@model IEnumerable<User>

@{
    List<Group> groups = ViewBag.Groups;
    ViewBag.Groups = groups;

}

<div class="tableContainer">
    <table>
        <tr>
            <th>
                Почта
            </th>
            <th>
                Фамилия
            </th>
            <th>
                Имя
            </th>
            <th>
                Группа
            </th>
            <th>
                Управление
            </th>
        </tr>
        @foreach (User user in Model) {
            @Html.Partial("Partial/_UserRow", user)
        }
    </table>
</div>